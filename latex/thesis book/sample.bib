@article{idx165,
title = {{Energy transfer in oxygen-hydrogen collisions}},
journal = {Planetary and Space Science},
volume = {32},
number = {7},
pages = {825-830},
year = {1984},
issn = {0032-0633},
doi = {https://doi.org/10.1016/0032-0633(84)90005-9},
url = {https://www.sciencedirect.com/science/article/pii/0032063384900059},
author = {D.L. Cooper and J.H. Yee and A. Dalgarno},
abstract = {A formula is obtained for the rate coefficient for the production of a particle with a specific energy in an elastic collision with the atoms of a thermal gas. Accurate calculations are carried out of the energy transfer in elastic collisions of oxygen and hydrogen atoms. The fractions of hydrogen atoms with sufficient energy to escape from Venus produced in collisions of oxygen atoms with energies of 2.6 eV in a gas of atomic hydrogen at 100, 200 and 300 K are respectively 5.1, 6.9 and 8.5 percent. The mutual diffusion coefficient D of oxygen and hydrogen atoms is obtained. It can be represented by Dn = 7.25 × 1017T0.71cm−1s−1 where n is the total atom density and T is the temperature.}
}

